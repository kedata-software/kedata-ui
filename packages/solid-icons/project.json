{
  "name": "solid-icons",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/solid-icons/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/kedark-solid-icons",
        "command": "tsx bin/sync.ts"
      }
    },
    "build": {
      "executor": "@kedata-web/tsup:build",
      "options": {
        "tsconfig": "tsconfig.json",
        "dts": true,
        "treeshaking": true,
        "external": ["solid"],
        "minify": false,
        "exports": {
          "require": "./index.cjs.jsx",
          "import": "./index.esm.jsx"
        },
        "isJsx": true
      }
    }
  }
}
